msgid "plugins.generic.citationManager.settings.status.button"
msgstr "Einstellungen testen"

msgid "plugins.generic.citationManager.not_implemented"
msgstr "Diese Funktion ist noch nicht implementiert."

msgid "plugins.generic.citationManager.settings.initialise.button"
msgstr "Plugin initialisieren"

msgid "plugins.generic.citationManager.settings.initialise.notification"
msgstr "Initialisierung des Plugins war erfolgreich"

msgid "plugins.generic.citationManager.settings.process.button"
msgstr "Batch process"

