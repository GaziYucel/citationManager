msgid "plugins.generic.optimetaCitationsPlugin.name"
msgstr "Optimeta Zitations-Plugin"

msgid "plugins.generic.optimetaCitationsPlugin.description"
msgstr "Optimeta Zitations-Plugin"

msgid "plugins.generic.optimetaCitationsPlugin.publicationStatement"
msgstr "Veröffentlichungserklärung"

msgid "plugins.generic.optimetaCitationsPlugin.publicationStatement.description"
msgstr "Fügen Sie eine Beschreibung der Publikation hinzu, die auf jeder Landing Page der Publikation erscheinen soll."

msgid "plugins.generic.optimetaCitationsPlugin.publication.label"
msgstr "Zitationen"

msgid "plugins.generic.optimetaCitationsPlugin.deposit.button"
msgstr "Übermitteln"

msgid "plugins.generic.optimetaCitationsPlugin.deposit.question"
msgstr "Sind Sie sich sicher, dass Sie die Zitationen an die oben genannten Websites übermitteln wollen?"

msgid "plugins.generic.optimetaCitationsPlugin.deposit.message.not_published"
msgstr "Die Publikation muss einer Ausgabe zugeordnet werden, benötigt einen DOI und muss veröffentlicht werden, bevor eine Übermittlung an die Websites möglich ist."

msgid "plugins.generic.optimetaCitationsPlugin.clear.button"
msgstr "Löschen"

msgid "plugins.generic.optimetaCitationsPlugin.clear.question"
msgstr "Die strukturierten Zitationen werden entfernt, wenn Sie auf OK klicken. Sind Sie sich sicher?"

msgid "plugins.generic.optimetaCitationsPlugin.citations.empty.description"
msgstr "Es sind keine strukturierten Zitationen vorhanden. Bitte klicken Sie auf den oben stehenden Button \"Verarbeiten\", um die Zitationen zu strukturieren und anzureichern."

msgid "plugins.generic.optimetaCitationsPlugin.process.button"
msgstr "Verarbeiten"

msgid "plugins.generic.optimetaCitationsPlugin.process.label"
msgstr "Strukturierte Literaturhinweise/Zitationen"

msgid "plugins.generic.optimetaCitationsPlugin.process.description"
msgstr "Bitte benutzen Sie den Button \"Verarbeiten\", um die Zitationen zu strukturieren und anzureichern.<br/>"
"Nach der Extraktion der DOIs werden zusätzliche Metadaten von externen Websites erfasst und unten angezeigt.<br/>"
"Die Übermittlung dieser Metadaten erfolgt automatisch, wenn dieser Beitrag veröffentlicht wird und einen DOI erhält. Alternativ kann die Metadatenübermittlung manuell über den Button \"Übermitteln\" erfolgen."

msgid "plugins.generic.optimetaCitationsPlugin.process.question"
msgstr "Die strukturierten Zitationen werden überschrieben, wenn Sie auf OK klicken. Alle Ihre Änderungen gehen dabei verloren. Sind Sie sich sicher?"

msgid "plugins.generic.optimetaCitationsPlugin.author.add.button"
msgstr "Autor hinzufügen"

msgid "plugins.generic.optimetaCitationsPlugin.author.remove.question"
msgstr "Sind Sie sich sicher, dass Sie diesen Autor löschen wollen?"

msgid "plugins.generic.optimetaCitationsPlugin.publication.success"
msgstr "Die Zitationen wurden aktualisiert."

msgid "plugins.generic.optimetaCitations.settings.description"
msgstr "Geben Sie Ihre Authentifizierungsinformationen für Open-Access-Websites ein, auf denen Sie Zitationen hinterlegen möchten."

msgid "plugins.generic.optimetaCitations.settings.form.saved"
msgstr "Einstellungen erfolgreich gespeichert"

msgid "plugins.generic.optimetaCitations.settings.wikidata_title"
msgstr "Wikidata"

msgid "plugins.generic.optimetaCitations.settings.wikidata_username"
msgstr "Wikidata Benutzername"

msgid "plugins.generic.optimetaCitations.settings.wikidata_password"
msgstr "Wikidata Passwort"

msgid "plugins.generic.optimetaCitations.settings.wikidata_api_url"
msgstr "Wikidata API URL"

msgid "plugins.generic.optimetaCitations.settings.open_citations_title"
msgstr "Open Citations CROCI <a href='https://github.com/TIBHannover/optimetaCitations/blob/main/README.md#registering-for-opencitationsorg-croci' target='_blank'><i class='fa fa-question-circle' aria-hidden='true'></i></a>"

msgid "plugins.generic.optimetaCitations.settings.open_citations_owner"
msgstr "Github-Besitzer für OpenCitations-Deposits"

msgid "plugins.generic.optimetaCitations.settings.open_citations_repository"
msgstr "Github-Repository-Name für OpenCitations-Deposits"

msgid "plugins.generic.optimetaCitations.settings.open_citations_token"
msgstr "Github Token für OpenCitations-Deposits"

msgid "plugins.generic.optimetaCitations.settings.test.button"
msgstr "Einstellungen testen"

msgid "plugins.generic.optimetaCitations.not_implemented"
msgstr "Diese Funktion ist noch nicht implementiert."

msgid "plugins.generic.optimetaCitations.settings.initialise.button"
msgstr "Plugin initialisieren"

msgid "plugins.generic.optimetaCitations.settings.initialise.notification"
msgstr "Initialisierung des Plugins war erfolgreich"

msgid "plugins.generic.optimetaCitations.settings.deposit.button"
msgstr "Publikation übermitteln"

msgid "plugins.generic.optimetaCitations.settings.deposit.notification"
msgstr "Alle veröffentlichten Publikationen sind übermittelt."
